<ivy-module version="1.0">
	<info organisation="Pivotal" module="PXF">
		<license name="Pivotal" />
		<ivyauthor name="PXF Hadoop Team" url="http://gopivotal.com" />
		<description>
			PFX Hadoop Integration
		</description>
