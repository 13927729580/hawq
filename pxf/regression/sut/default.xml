<sut>
	<cluster>
		<class>com.pivotal.parot.components.cluster.SingleCluster</class>
		<host>localhost</host>
	</cluster>

	<hawq>
		<class>com.pivotal.parot.components.hawq.Hawq</class>
		<host>localhost</host>
		<db>pxf_regression</db>
	</hawq>

	<hdfs>
		<class>com.pivotal.parot.components.hdfs.Hdfs</class>
		<host>localhost</host>
		<port>8020</port>
		<workingDirectory>pxf_regression_data</workingDirectory>
	</hdfs>

	<hbase>
		<class>com.pivotal.parot.components.hbase.HBase</class>
		<host>localhost</host>
	</hbase>

	<hive>
		<class>com.pivotal.parot.components.hive.Hive</class>
		<host>localhost</host>
		<port>10004</port>
	</hive>

	<tinc>
		<class>com.pivotal.parot.components.tinc.Tinc</class>
		<host>localhost</host>
		<gphome>$GPHOME</gphome>
		<tincFolder>/Users/koazu/work/workspace/tinc/main</tincFolder>
		<tincTestsFolder>/Users/koazu/work/workspace/tincrepo/main</tincTestsFolder>
	</tinc>

	<shellsystemobject>
		<JAVA_HOME></JAVA_HOME>
		<GPHOME></GPHOME>
		<GPHD_ROOT></GPHD_ROOT>
		<GPDATA></GPDATA>
		<MASTER_DATA_DIRECTORY></MASTER_DATA_DIRECTORY>
		<PGPORT></PGPORT>
		<PGHOST></PGHOST>
	</shellsystemobject>
</sut>
